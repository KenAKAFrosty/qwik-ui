    "@nrwl/cli": "14.8.3",
    "@nrwl/cypress": "15.0.8",
    "@nrwl/eslint-plugin-nx": "14.8.3",
    "@nrwl/linter": "14.8.3",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/storybook": "15.0.8",
    "@nrwl/workspace": "14.8.3",
    "@storybook/addon-essentials": "~6.5.9",
